  <about>
    <name>fnbreak</name>
    <caption>Warn for splitted footnotes.</caption>
    <author>
      <name>Harald Harders</name>
      <email>h.harders@tu-bs.de</email>
    </author>
    <license type="lppl"/>
    <version>
      <number>1.10</number>
